<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="main.dao.TeacherDAO">

    <select id="getTeacherByUsername" resultType="main.model.Teacher">
        select * from teacher WHERE username = #{username}
    </select>

    <select id="getAllTeacher" resultType="main.model.Teacher">
        select * from teacher
    </select>

</mapper>